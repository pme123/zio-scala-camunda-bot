var from = json.prop("from");
var username = from.prop("username");

var botTaskJson = {
ident: issueIdent,
chatUserOrGroup:"camunda_group",
msg: username.value() + " needs help with "+ issueIdent,
maybeCallback:{
  messageName: "taskClaimed",
  businessKey: businessKey,
  controls:[{
   ident:"claimed",
   text:"I claim the issue",
